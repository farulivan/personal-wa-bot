# WhatsApp Bot Configuration
# Copy this file to .env and fill in your values

# Comma-separated list of phone numbers allowed to use the bot
# Format: country code + number, no spaces or symbols
# Example: 6281234567890,6289876543210
ALLOWED_NUMBERS=

# Flag for debugging (boolean)
DEBUG=

# Reset WhatsApp auth session (boolean). Set true to force new QR pairing.
RESET_AUTH=
